{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "types": ["npm:@types/react", "npm:@types/react-dom"]
  },
  "lint": {
    "exclude": ["src/routeTree.gen.ts"]
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@/": "./src/",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@radix-ui/react-dialog": "npm:@radix-ui/react-dialog@^1.1.4",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.4",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.1",
    "@radix-ui/react-separator": "npm:@radix-ui/react-separator@^1.1.1",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.1.1",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.1.6",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.63.0",
    "@tanstack/react-query-devtools": "npm:@tanstack/react-query-devtools@^5.63.0",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.95.2",
    "@tanstack/router-devtools": "npm:@tanstack/router-devtools@^1.95.2",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.95.2",
    "@types/react": "npm:@types/react@^19.0.4",
    "@types/react-dom": "npm:@types/react-dom@^19.0.2",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.7.2",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "axios": "npm:axios@^1.7.9",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "lucide-react": "npm:lucide-react@^0.469.0",
    "radix-ui": "npm:radix-ui@^1.0.1",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^18.3.1",
    "tailwind-merge": "npm:tailwind-merge@^2.6.0",
    "tailwindcss": "npm:tailwindcss@^3.4.17",
    "tailwindcss-animate": "npm:tailwindcss-animate@^1.0.7",
    "vite": "npm:vite@^6.0.1"
  }
}
